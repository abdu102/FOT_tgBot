{
  "name": "fot",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc -p tsconfig.json",
    "prestart": "prisma generate && prisma migrate deploy",
    "start": "node dist/index.js",
    "preinstall": "rm -rf node_modules/.cache || true",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:seed": "ts-node prisma/seed.ts",
    "format": "prettier --write .",
    "postinstall": ""
  },
  "engines": {
    "node": "^20.0.0"
  },
  "packageManager": "npm@10",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@types/node": "^24.3.0",
    "bcryptjs": "^3.0.2",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "express": "^4.19.2",
    "node-cron": "^4.2.1",
    "prisma": "^6.14.0",
    "telegraf": "^4.16.3",
    "telegraf-i18n": "^6.6.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "nodemon": "^3.1.10"
  }
}
